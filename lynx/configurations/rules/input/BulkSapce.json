{
  "_DATE": 20201031,
  "_AUTHORS": [
    "zhixu.ni@uni-leipzig.de"
  ],
  "SOURCES": [
    "BulkSpace"
  ],
  "SEPARATOR": {
    "LIPID_CLASS_SEPARATOR": "\\s",
    "MOD_SEPARATOR": "-",
    "SPACE": "\\s",
    "COLON": ":",
    "SEMICOLON": ";",
    "SEPARATOR_LEVELS": {
      "B": ""
    }
  },
  "RESIDUE": {
    "RESIDUE_INFO": {
      "ORDER": [
        "LINK",
        "C_COUNT",
        "DB_SEPARATOR",
        "DB_COUNT"
      ],
      "OPTIONAL": [
        "LINK",
        "DB_COUNT"
      ],
      "LINK": "[OP]-|C|[mdt]|h",
      "C_COUNT": "[0-9]{1,2}",
      "DB_SEPARATOR": "{{SEPARATOR.COLON}}",
      "DB_COUNT": "[1,2]?[0-9]",
      "EXAMPLES": [
        "18:2",
        "9:0"
      ]
    },
    "RESIDUE_ALIAS": {
      "ORDER": [
        "ALIAS"
      ],
      "RESIDUE_SEPARATOR": "{{SEPARATOR.RESIDUE_SEPARATOR}}",
      "ALIAS": "[-,0-9a-zA-Z\\s]{3,}",
      "EXAMPLES": [
        "HETE",
        "/HDoTE"
      ]
    },
    "RESIDUE_INFO_SUM": {
      "ORDER": [
        "LIPID_CLASS_SEPARATOR",
        "AVOID_RESIDUE_SEPARATOR",
        "RESIDUE_INFO"
      ],
      "LIPID_CLASS_SEPARATOR": "{{SEPARATOR.LIPID_CLASS_SEPARATOR}}",
      "AVOID_RESIDUE_SEPARATOR": "(?!_)|(?!/)",
      "RESIDUE_INFO": "{{RESIDUE.RESIDUE_INFO}}",
      "EXAMPLES": [
        "18:2;3O",
        "18:2;O",
        "18:2",
        "9:0",
        "26:0;(2OH)"
      ]
    }
  },
