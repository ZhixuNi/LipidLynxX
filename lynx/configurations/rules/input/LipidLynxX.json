      "B": "",
      "M": "_",
      "S": "/"
    }
  },
  "SITE": {
    "SITE_INFO": {
      "ORDER": [
        "SITE",
        "SITE_SUFFIX"
      ],
      "OPTIONAL": [
        "SITE_SUFFIX"
      ],
      "SITE": "(\\s?,)?([0-9]{1,2})",
      "SITE_SUFFIX": "([EZRS])?",
      "EXAMPLES": [
        "12",
        "12Z"
      ]
    },
    "SITE_INFO_SUM": {
      "ORDER": [
        "SITE_BRACKET_LEFT",
        "SITE_INFO",
        "SITE_BRACKET_RIGHT"
      ],
      "REPEAT": {
        "SITE_INFO": [1, 10]
      },
      "SITE_BRACKET_LEFT": "{{SEPARATOR.SITE_BRACKET_LEFT}}",
      "SITE_INFO": "{{SITE.SITE_INFO}}",
      "SITE_BRACKET_RIGHT": "{{SEPARATOR.SITE_BRACKET_RIGHT}}",
      "EXAMPLES": [
        "{9Z,12Z}"
      ]
    }
  },
